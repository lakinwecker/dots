Xft.antialias:  1
Xft.autohint:   false
Xft.dpi:        96
Xft.hinting:    1
Xft.hintstyle:  hintfull
Xft.lcdfilter:  lcddefault
Xft.rgba:       rgb


! -----------------------------
! ! URXVT OPTIONS
! ! -----------------------------

URxvt*borderless:         true
URxvt*internalBorder:     15
URxvt*jumpScroll:         true
URxvt*scrollBar:          false
URxvt*scrollTryOutput:    false
URxvt*scrollTtyKeypress:  true
URxvt*urgentOnBell:       true
URxvt*visualBell:         false
URxvt*letterSpace:        -1
URxvt*matcher.button:     1
urxvt*depth: 32
urxvt*background: rgba:eeee/eeee/eeee/d000
URxvt.cursorColor:        #4D4D4C
URxvt*foreground:         black
URxvt.perl-ext-common:    selection-to-clipboard


! ! -----------------------------
! ! FONTS
! ! -----------------------------

URxvt.font: xft:Fira Mono Medium:size=12,xft:Inconsolata\ for\ Powerline:size=12
URxvt.boldFont: xft:Fira Mono Medium:size=12,xft:Inconsolata\ for\ Powerline:size=12
URxvt.italicFont: xft:Fira Mono Medium:size=12,xft:Inconsolata\ for\ Powerline:size=12
URxvt.boldItalicFont: xft:Fira Mono Medium:size=12,xft:Inconsolata\ for\ Powerline:size=12



! PaperColor Theme
URxvt.foreground: #4D4D4C
URxvt.background: #EEEEEE

! black
URxvt.color0: #EDEDED
URxvt.color8: #969694

! red
URxvt.color1: #D7005F
URxvt.color9: #D7005F

! green
URxvt.color2: #718C00
URxvt.color10: #718C00

! yellow / orange
URxvt.color3: #D75F00
URxvt.color11: #D75F00

! blue
URxvt.color4: #4271AE
URxvt.color12: #4271AE

! magenta
URxvt.color5: #8959A8
URxvt.color13: #8959A8

! cyan
URxvt.color6: #3E999F
URxvt.color14: #3E999F

! white
URxvt.color7: #4D4D4C
URxvt.color15: #F5F5F5
